{% set services = [
  { 'service': 'print', 'link': 'javascript:window.print();', 'text': 'Send to Printer'},
  { 'service': 'facebok', 'link': '//www.facebook.com/sharer.php', 'text': 'Share on Facebook'},
  { 'service': 'twitter', 'link': '//www.twitter.com/share', 'text': 'Share on Twitter'},
  { 'service': 'email', 'link': '/email', 'text': 'Email this story'},
  { 'service': 'plus', 'link': '/more', 'text': 'More Options'}
] %}

<div class="social-links share">
  {% for linkdata in services %}
  <li class="{{ linkdata.service }}">
    <a href="{{ linkdata.link }}" title="{{ linkdata.text }}">{{ linkdata.text }}</a>
  </li>
  {% endfor %}
</div>
